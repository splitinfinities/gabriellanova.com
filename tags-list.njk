---
permalink: /tags/
layout: layouts/home.njk
---
<h1>Tags</h1>

{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
  <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
{% endfor %}

<midwest-theme base="orange" complement="violet" inert="inert" system></midwest-theme>

<midwest-layout type="five-column" align="top" size="full" class="fill">
  {% set postslist = entries %}
  {% include "postslist.njk" %}
</midwest-layout>